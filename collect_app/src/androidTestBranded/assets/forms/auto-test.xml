<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Automated Testing</h:title>
    <model>
      <instance>
        <data id="snapshot_xml">
          <start/>
          <end/>
          <today/>
          <username/>
          <deviceid/>
          <New_Question/>
          <group_ro1iv44>
            <Coliforms/>
            <group_coliforms>
              <Result/>
              <Sample_volume/>
              <Broth/>
              <Time_to_detect/>
            </group_coliforms>
          </group_ro1iv44>
          <group_zc3re54>
            <Arsenic_0_500/>
            <Arsenic_0_4000/>
            <group_arsenic>
              <Arsenic/>
              <Arsenic_Unit/>
            </group_arsenic>
          </group_zc3re54>
          <group_hw5xj60>
            <pH/>
            <Available_Nitrogen/>
            <Available_Phosphorous/>
          </group_hw5xj60>
          <group_hr02n72>
            <Available_Potassium/>
            <Available_Boron/>
            <Available_Iron/>
          </group_hr02n72>
          <group_lq9xd22>
            <group_calmag>
              <Exchangeable_Calcium_001/>
              <Exchangeable_Magnesium_001/>
            </group_calmag>
          </group_lq9xd22>
          <group_pg7xj25>
            <Arsenic_III/>
            <Chromium/>
            <Fluoride/>
          </group_pg7xj25>
          <group_et5co41>
            <Free_Chlorine/>
            <Nitrate/>
            <pH_001/>
          </group_et5co41>
          <group_ep33t57>
            <Phosphate/>
            <group_fluoride>
              <Fluoride_xBorewell/>
              <Fluoride_Unit_xBorewell/>
              <Fluoride_Dilution_xBorewell/>
            </group_fluoride>
            <Total_Iron/>
          </group_ep33t57>
          <__version__/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username" type="string"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/>
      <bind nodeset="/data/New_Question" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/data/group_ro1iv44/Coliforms" required="false()" type="string"/>
      <bind nodeset="/data/group_ro1iv44/group_coliforms/Result" required="false()" type="string"/>
      <bind nodeset="/data/group_ro1iv44/group_coliforms/Sample_volume" required="false()" type="string"/>
      <bind nodeset="/data/group_ro1iv44/group_coliforms/Broth" required="false()" type="string"/>
      <bind nodeset="/data/group_ro1iv44/group_coliforms/Time_to_detect" required="false()" type="string"/>
      <bind nodeset="/data/group_zc3re54/Arsenic_0_500" required="false()" type="string"/>
      <bind nodeset="/data/group_zc3re54/Arsenic_0_4000" required="false()" type="string"/>
      <bind nodeset="/data/group_zc3re54/group_arsenic/Arsenic" required="false()" type="string"/>
      <bind nodeset="/data/group_zc3re54/group_arsenic/Arsenic_Unit" required="false()" type="string"/>
      <bind nodeset="/data/group_hw5xj60/pH" required="false()" type="string"/>
      <bind nodeset="/data/group_hw5xj60/Available_Nitrogen" required="false()" type="string"/>
      <bind nodeset="/data/group_hw5xj60/Available_Phosphorous" required="false()" type="string"/>
      <bind nodeset="/data/group_hr02n72/Available_Potassium" required="false()" type="string"/>
      <bind nodeset="/data/group_hr02n72/Available_Boron" required="false()" type="string"/>
      <bind nodeset="/data/group_hr02n72/Available_Iron" required="false()" type="string"/>
      <bind nodeset="/data/group_lq9xd22/group_calmag/Exchangeable_Calcium_001" required="false()" type="string"/>
      <bind nodeset="/data/group_lq9xd22/group_calmag/Exchangeable_Magnesium_001" required="false()" type="string"/>
      <bind nodeset="/data/group_pg7xj25/Arsenic_III" required="false()" type="string"/>
      <bind nodeset="/data/group_pg7xj25/Chromium" required="false()" type="string"/>
      <bind nodeset="/data/group_pg7xj25/Fluoride" required="false()" type="string"/>
      <bind nodeset="/data/group_et5co41/Free_Chlorine" required="false()" type="string"/>
      <bind nodeset="/data/group_et5co41/Nitrate" required="false()" type="string"/>
      <bind nodeset="/data/group_et5co41/pH_001" required="false()" type="string"/>
      <bind nodeset="/data/group_ep33t57/Phosphate" required="false()" type="string"/>
      <bind nodeset="/data/group_ep33t57/group_fluoride/Fluoride_xBorewell" required="false()" type="string"/>
      <bind nodeset="/data/group_ep33t57/group_fluoride/Fluoride_Unit_xBorewell" required="false()" type="string"/>
      <bind nodeset="/data/group_ep33t57/group_fluoride/Fluoride_Dilution_xBorewell" required="false()" type="int"/>
      <bind nodeset="/data/group_ep33t57/Total_Iron" required="false()" type="string"/>
      <bind calculate="'vkMFBR8pdkXJJJTo4fLNBT'" nodeset="/data/__version__" required="false()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/New_Question">
      <label>New Question</label>
      <hint>&lt;br/&gt;Welcome to automated testing survey form!&lt;br/&gt;&lt;br/&gt;This is version number 11</hint>
    </input>
    <group appearance="field-list" ref="/data/group_ro1iv44">
      <label>Group</label>
      <input appearance="ex:io.ffem.experiment(testId='df3d1009-2112-4d95-a6f9-fdc4b5633ec9')" ref="/data/group_ro1iv44/Coliforms">
        <label>Coliforms 1</label>
      </input>
      <group appearance="field-list" intent="io.ffem.experiment(testId='df3d1009-2112-4d95-a6f9-fdc4b5633ec9')" ref="/data/group_ro1iv44/group_coliforms">
        <label>Coliforms 2</label>
        <input ref="/data/group_ro1iv44/group_coliforms/Result">
          <label>Result</label>
        </input>
        <input ref="/data/group_ro1iv44/group_coliforms/Sample_volume">
          <label>Sample volume</label>
        </input>
        <input ref="/data/group_ro1iv44/group_coliforms/Broth">
          <label>Broth</label>
        </input>
        <input ref="/data/group_ro1iv44/group_coliforms/Time_to_detect">
          <label>Time to detect</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/group_zc3re54">
      <label>Arsenic</label>
      <input appearance="ex:io.ffem.water(testId='32b6d11b-c7f3-475f-adf9-32d9b8f4aecf')" ref="/data/group_zc3re54/Arsenic_0_500">
        <label>Arsenic (0 - 500)</label>
      </input>
      <input appearance="ex:io.ffem.water(testId='beca9731-63f4-434f-a3c2-ac33b44f9992')" ref="/data/group_zc3re54/Arsenic_0_4000">
        <label>Arsenic (0 - 4000)</label>
      </input>
      <group appearance="field-list" intent="io.ffem.water(testId='32b6d11b-c7f3-475f-adf9-32d9b8f4aecf')" ref="/data/group_zc3re54/group_arsenic">
        <label>Arsenic (0 - 500)</label>
        <input ref="/data/group_zc3re54/group_arsenic/Arsenic">
          <label>Arsenic</label>
        </input>
        <input ref="/data/group_zc3re54/group_arsenic/Arsenic_Unit">
          <label>Unit</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/group_hw5xj60">
      <label>Soil Tests 1</label>
      <input appearance="ex:io.ffem.soil(testId='485a0c0a-0af5-40bc-baee-f5f0b03ff739')" ref="/data/group_hw5xj60/pH">
        <label>pH</label>
      </input>
      <input appearance="ex:io.ffem.soil(testId='8e7badf4-3c7e-4c45-9cb2-0810ffaa8c26')" ref="/data/group_hw5xj60/Available_Nitrogen">
        <label>Available Nitrogen</label>
      </input>
      <input appearance="ex:io.ffem.soil(testId='ddd64497-43e0-4289-a84f-6890ff80cf20')" ref="/data/group_hw5xj60/Available_Phosphorous">
        <label>Available Phosphorous</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/group_hr02n72">
      <label>Soil Tests 2</label>
      <input appearance="ex:io.ffem.soil(testId='0035a4fc-6b9b-4b12-975f-3459c7fdcda3')" ref="/data/group_hr02n72/Available_Potassium">
        <label>Available Potassium</label>
      </input>
      <input appearance="ex:io.ffem.soil(testId='29e609a0-0f57-4365-a4eb-4eb84c61c9ab')" ref="/data/group_hr02n72/Available_Boron">
        <label>Available Boron</label>
      </input>
      <input appearance="ex:io.ffem.soil(testId='3353f5cf-1cd2-4bf5-b47f-15d3db917add')" ref="/data/group_hr02n72/Available_Iron">
        <label>Available Iron</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/group_lq9xd22">
      <label>Soil Tests 3</label>
      <group appearance="field-list" intent="io.ffem.soil(testId='52ec4ca0-d691-4f2b-b17a-232c2966974a')" ref="/data/group_lq9xd22/group_calmag">
        <label>Exchangeable Calcium &amp; Magnesium</label>
        <input ref="/data/group_lq9xd22/group_calmag/Exchangeable_Calcium_001">
          <label>Exchangeable Calcium</label>
        </input>
        <input ref="/data/group_lq9xd22/group_calmag/Exchangeable_Magnesium_001">
          <label>Exchangeable Magnesium</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/group_pg7xj25">
      <label>Water Tests 1</label>
      <input appearance="ex:io.ffem.water(testId='93c259c5-f8b5-4317-828b-adfa324a2918')" ref="/data/group_pg7xj25/Arsenic_III">
        <label>Arsenic (III)</label>
      </input>
      <input appearance="ex:io.ffem.water(testId='d488672f-9a4c-4aa4-82eb-8a95c40d0296')" ref="/data/group_pg7xj25/Chromium">
        <label>Chromium</label>
      </input>
      <input appearance="ex:io.ffem.water(testId='f0f3c1dd-89af-49f1-83e7-bcc31c3006cf')" ref="/data/group_pg7xj25/Fluoride">
        <label>Fluoride</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/group_et5co41">
      <label>Water Tests 2</label>
      <input appearance="ex:io.ffem.water(testId='a2413119-38eb-4959-92ee-cc169fdbb0fc')" ref="/data/group_et5co41/Free_Chlorine">
        <label>Free Chlorine</label>
      </input>
      <input appearance="ex:io.ffem.water(testId='c08123f7-eaa6-44d0-9113-1c16bf4b183b')" ref="/data/group_et5co41/Nitrate">
        <label>Nitrate</label>
      </input>
      <input appearance="ex:io.ffem.water(testId='4d3a7d6d-2f11-49de-9f7f-9e21e840df93')" ref="/data/group_et5co41/pH_001">
        <label>pH</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/group_ep33t57">
      <label>Water Tests 3</label>
      <input appearance="ex:io.ffem.water(testId='6fbe9952-7199-4c9d-8664-977236176cc9')" ref="/data/group_ep33t57/Phosphate">
        <label>Phosphate</label>
      </input>
      <group appearance="field-list" intent="io.ffem.water(testId='f0f3c1dd-89af-49f1-83e7-bcc31c3006cf')" ref="/data/group_ep33t57/group_fluoride">
        <label>Fluoride - Borewell</label>
        <input ref="/data/group_ep33t57/group_fluoride/Fluoride_xBorewell">
          <label>Fluoride</label>
        </input>
        <input ref="/data/group_ep33t57/group_fluoride/Fluoride_Unit_xBorewell">
          <label>Unit</label>
        </input>
        <input ref="/data/group_ep33t57/group_fluoride/Fluoride_Dilution_xBorewell">
          <label>Dilution</label>
        </input>
      </group>
      <input appearance="ex:io.ffem.water(testId='5a7cf06e-5e73-4044-9aa5-0206cc627c82')" ref="/data/group_ep33t57/Total_Iron">
        <label>Total Iron</label>
      </input>
    </group>
  </h:body>
</h:html>
