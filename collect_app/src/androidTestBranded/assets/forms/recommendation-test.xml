      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind nodeset="/data/group_lh11l41/Date" required="false()" type="date"/>
      <bind nodeset="/data/group_lh11l41/Farmer_name" required="false()" type="string"/>
      <bind nodeset="/data/group_lh11l41/Phone_number" required="false()" type="string"/>
      <bind nodeset="/data/group_fh2sw09/Village_name" required="false()" type="string"/>
      <bind nodeset="/data/group_fh2sw09/Geolocation" required="false()" type="geopoint"/>
