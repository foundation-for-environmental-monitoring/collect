        </input>
        <input ref="/data/group_zq0ad92/group_available_potassium/Available_Potassium_Unit">
          <label>Unit</label>
        </input>
        <input ref="/data/group_zq0ad92/group_available_potassium/Available_Potassium_Dilution">
          <label>Dilution</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/group_fi5nk42">
      <label>Crop Info</label>
      <select1 ref="/data/group_fi5nk42/State">
