      <input ref="/data/group_crop_recommendation/Comb2Fert1">
        <label>Fertilizer</label>
      </input>
      <input ref="/data/group_crop_recommendation/Comb2Value1">
        <label>Dosage</label>
      </input>
      <input ref="/data/group_crop_recommendation/Comb2Fert2">
