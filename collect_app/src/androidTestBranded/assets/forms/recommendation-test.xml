  <h:body>
    <group appearance="field-list" ref="/data/group_lh11l41">
      <label>Info</label>
      <input ref="/data/group_lh11l41/Date">
        <label>Date</label>
      </input>
      <input ref="/data/group_lh11l41/Farmer_name">
        <label>Farmer name</label>
      </input>
      <input appearance="numbers" ref="/data/group_lh11l41/Phone_number">
        <label>Phone number</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/group_fh2sw09">
      <label>Info 2</label>
      <input ref="/data/group_fh2sw09/Village_name">
